{
  "name": "these-kids-read",
  "version": "2.0.0",
  "description": "Our family reads great books together",
  "scripts": {
    "watch:sass": "sass --no-source-map --watch src/_scss:_site/css",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "sass --no-source-map src/_scss:_site/css",
    "build:eleventy": "eleventy",
    "build:postcss": "postcss _site/css/*.css -u autoprefixer cssnano -r --no-map",
    "dev": "npm-run-all build:sass --parallel watch:sass watch:eleventy",
    "build": "npm-run-all build:sass build:eleventy build:postcss"
  },
  "author": "Jared Pendergraft <jaredpdesigns@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@netlify/functions": "^1.0.0",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.8",
    "fs-extra": "^11.3.0",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "precss": "^4.0.0",
    "sass": "^1.51.0",
    "terser": "^5.13.1"
  },
  "packageManager": "yarn@4.6.0+sha512.5383cc12567a95f1d668fbe762dfe0075c595b4bfff433be478dbbe24e05251a8e8c3eb992a986667c1d53b6c3a9c85b8398c35a960587fbd9fa3a0915406728"
}
